package com.instanceInitilizer;

public class InstanceInitilizerBlock {
	
	public InstanceInitilizerBlock() {

	System.out.println("Parent class   Constructor()"); // Execute the first
	}

}
class SubClass extends InstanceInitilizerBlock{
	

	public SubClass() {
		super();
		
		System.out.println(" sub Class Constructor()"); // execute third

		
		{
			System.out.println(" This is the Instance InitilizerBlock"); // Execute secound
		}
		
	};
	
	public static void main(String[] args) {
		
		InstanceInitilizerBlock e = new InstanceInitilizerBlock();
		
		
	}
	
}
