package com.mathClass;

import java.util.Random;

public class RandomNumber {

	public static void main(String[] args) {
		
		Random num = new Random();
		/*
		 * for(int i=0; i<1; i++) {
		 * 
		 * 
		 * System.out.println(num.nextInt(100)); }
		 */
		
		int i =0;
		while(i<1) {
			System.out.println(num.nextInt(1000000));
			i++;
		}
	}
}
