package com.exceptionHandiling;

public class MultiCatachBlock {
	
	public int multicath() {
		try {
		int i =10;
		int j =0;
		
	}
		catch (ArithmeticException e) {
		}
		
		return  i/j;

	}


public static void main(String[] args) {
	
	MultiCatachBlock m = new MultiCatachBlock();
	m.multicath();
}
}
