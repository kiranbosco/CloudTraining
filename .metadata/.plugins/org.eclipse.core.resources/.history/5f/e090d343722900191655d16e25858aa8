package com.overloading;

public class TypePromote {

	int sum (int a, int b) {
		
		return a+b;
	}
	
	float sum(float a, float b) {
		
		return a+b;
	}
	
	public static void main(String[] args) {
		
		TypePromote t = new TypePromote();
		t.sum(10.5f, 5.5f);
		t.sum(20, 10);
		System.out.println(t);
	}
}
