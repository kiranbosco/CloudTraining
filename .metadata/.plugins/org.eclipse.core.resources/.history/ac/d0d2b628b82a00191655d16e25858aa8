package com.basic;

import java.util.ArrayList;
import java.util.List;

public class FindIntegerDuplicates {

	public static void main(String[] args) {

//Convert the arrys into arraylsit
		
		List<Integer> al = new ArrayList<Integer>(); 
        al.add(10); 
        al.add(20); 
        al.add(30); 
        al.add(40); 
        
int [] arr = al.toArray();        
	 
} 
}