package com.collections;

import java.util.ArrayList;
import java.util.Collection;
import java.util.Collections;

public class UnmodifyableClass {

	public static void main(String[] args) {


		Collection readOnlyCollection = Collections.unmodifiableCollection(new ArrayList<String>());
        readOnlyCollection.add("Sydney Sheldon"); //raises
        readOnlyCollection.add("us Sheldon"); //raises
        readOnlyCollection.add("uk Sheldon"); //raises


	}

}
