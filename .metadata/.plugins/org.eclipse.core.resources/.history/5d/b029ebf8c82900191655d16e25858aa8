package com.mathClass;

import java.util.Random;

public class RandomNumber {

	public static void main(String[] args) {
		
		Random num = new Random();
		/*
		 * for(int i=0; i<1; i++) {
		 * 
		 * 
		 * System.out.println(num.nextInt(100)); }
		 */
		
		int i =0;
		while(i<1) {
			System.out.println( "Ur otp is + " + num.nextInt(1000000));
			i++;
		}
		
		String s = "12";
		Integer in = Integer.valueOf(s);
		System.out.println( "String convert To Integer " + in);
		
		Integer number =10;
		String ss = String.valueOf(number);
		System.out.println(ss);
		
		Integer sr = 100;
		Float f = Float.valueOf(sr);
		System.out.println(f);
		
		Float ff = 20.5f;
		
		String sf = String.valueOf(ff);
		System.out.println(sf);
		
		String sb = "kiran";
		Boolean bs = Boolean.valueOf(sb);
		System.out.println(bs);
		
		System.out.println();
		
		String name ="Bosco";
		char [] ch = name.toCharArray();
		for(int len=ch.length-1;len>=0; len--) {
			System.out.println(ch[len]);
		}
	
		// NumberFormate Exception
		
		String numf = "ABC";
		Integer ii = Integer.valueOf(numf);
		System.out.println(ii);
		
		// Null pointerException
		
		String nul =null;
	System.out.println(nul.length());
	
	// ArrayIndexOutOfBoundException
	
	int a[] =new int [10];
	a[10] =50;
	System.out.println(a);
	
	}
}
