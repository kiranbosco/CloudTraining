package com.collections;

public class DataStructor {
	
	public static void main(String[] args) {
		
		
		String s = "12354212";
		String[] ss =s.split("\\3");
		System.out.println(ss);
				
		
		
		String splt ="java is very is very is very is very is very is";
		String[] spt =splt.split("\\s");
		for(String ss : spt) {
			System.out.println(ss);
			System.out.println();
		
	}

}
