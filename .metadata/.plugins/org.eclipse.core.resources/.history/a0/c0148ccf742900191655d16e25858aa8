package com.instanceInitilizer;

public class InstanceInitilizerBlock {
	
	public InstanceInitilizerBlock() {

	System.out.println("Parent class   Constructor()");
	}

}
class SubClass extends InstanceInitilizerBlock{
	

	public SubClass() {
		super();
		
		System.out.println(" sub Class Constructor()");

		
		{
			System.out.println(" This is the Instance InitilizerBlock");
		}
		
	};
	
}
