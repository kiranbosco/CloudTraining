package com.collections;

import java.util.ArrayList;
import java.util.List;

public class GetList {
	
	public static void main(String[] args) {
		
		List list = new ArrayList<>();
		list.add("kiran");
		list.add("vijay");
		list.add("sujatha");
		
		System.out.println(list.get(0));
		
		
		
		
	}

}
